{
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
        "./llm-gateway" : {
            "enablePresidio" : false,
            "enableRedis" : true,
            "enableLangfuse" : true,
            "startWithDefaults": false
        }
    },
    "portsAttributes": {
        "3000": {"label": "Langfuse"},
        "4000": {"label": "LiteLLM"},
        "5001": {"label": "Presidio Analyzer"},
        "5002": {"label": "Presidio Anonymizer"},
        "5432": {"label": "Postgresql"},
        "6379": {"label": "Redis-stack"}
    }
}
